{
  "src": [
    "./lib/**/*.js",
    "./*.md"
  ],
  "clean": true,
  "dest": "./docs",
  "jsdoc": {
    "allowUnknownTags": true,
    "hierarchy": true,
    "recurse": true,
    "sort": "grouped",
    "plugins": [
      "plugins/markdown"
    ],
    "markdown": {
      "gfm": true
    }
  },
  "markdown": {
    "gfm": true,
    "tables": true,
    "tasks": true,
    "emoji": true
  },
  "app": {
    "title": "Mini-utils documentation",
    "base": "/mini-utils",
    "server": "github",
    "entrance": "content:readme"
  },
  "template": {
    "path": "default",
    "options": {
      "title": "mini-service-utils",
      "sidebar": {
        "itemsFolded": true
      },
      "symbols": {
        "params": "table"
      },
      "navbar": {
        "menu": [{
          "iconClass": "far fa-compass fa-lg",
          "label": "Welcome",
          "href": "?content=readme"
        }, {
          "iconClass": "fas fa-book fa-lg",
          "label": "API Reference",
          "href": "?api"
        }, {
          "iconClass": "fas fa-fire fa-lg",
          "label": "Changelog",
          "href": "?content=changelog"
        }, {
          "iconClass": "fab fa-github fa-lg",
          "label": "GitHub",
          "href": "https://github.com/feugy/mini-utils",
          "target": "_blank"
        }]
      }
    }
  }
}